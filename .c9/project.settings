{
    "ace": {
        "@tabSize": 2
    },
    "build": {
        "@saveall": true
    },
    "format": {
        "jsbeautify": {
            "@jslinthappy": false
        }
    },
    "golang": {
        "@completion": false
    },
    "javascript": {
        "@use_jsbeautify": false
    },
    "language": {
        "tern_defs": {
            "json()": {
                "browser": {
                    "enabled": true
                },
                "ecma5": {
                    "enabled": true
                },
                "jQuery": {
                    "enabled": true
                }
            }
        }
    },
    "php": {
        "@completion": false
    },
    "python": {
        "@completion": false
    },
    "run": {
        "configs": {
            "@inited": "true",
            "json()": {
                "Deploy Functions": {
                    "command": "",
                    "name": "Deploy Functions",
                    "runner": "function-deploy",
                    "toolbar": false
                },
                "Deploy API": {
                    "command": "",
                    "name": "Deploy API",
                    "runner": "api-deploy",
                    "toolbar": false
                },
                "Pi Server": {
                    "command": "",
                    "name": "Pi Server",
                    "runner": "pi",
                    "toolbar": false
                }
            }
        }
    },
    "welcome": {
        "@first": true
    }
}